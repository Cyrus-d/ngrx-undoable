{
  "name": "ngrx-undoable",
  "version": "1.0.0",
  "main": "lib/index.js",
  "repository": "git@gitlab.uni-koblenz.de:jannic/ngrx-undoable.git",
  "author": "jannicbeck <jannicbeck@gmail.com>",
  "license": "MIT",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "scripts": {
    "clean": "rimraf lib dist esnext coverage",
    "compile": "tsc src/undoable.reducer.ts",
    "build:commonjs": "tsc src/index.ts --outDir lib --module commonjs",
    "build:esnext": "tsc src/index.ts --outDir esnext --module esnext",
    "build:umd": "NODE_ENV=development rollup -c -i src/index.ts -o dist/redux-undoable.js",
    "build:umd:min": "NODE_ENV=production rollup -c -i src/index.ts -o dist/redux-undoable.min.js",
    "build": "yarn build:commonjs && yarn build:esnext && yarn build:umd && yarn build:umd:min",
    "test": "jest",
    "test:watch": "yarn test -- --watch",
    "test:cov": "yarn test -- --coverage"
  },
  "keywords": [
    "redux",
    "undo",
    "redo",
    "undoable",
    "undo-redo",
    "redux-undo",
    "react-undo",
    "ngrx-undo",
    "angular-undo",
    "angular2-undo"
  ],
  "devDependencies": {
    "@types/jest": "^21.1.6",
    "jest": "^21.2.1",
    "rollup": "^0.50.0",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-uglify": "^2.0.1",
    "ts-jest": "^21.1.0",
    "typescript": "^2.5.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/test/.*\\.test.(ts|tsx|js)$"
  }
}